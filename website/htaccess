# Note that the below is specific to patricksanan.org
RewriteEngine on
RewriteCond %{HTTP_HOST} ^patricksanan\.org$ [OR]
RewriteCond %{HTTP_HOST} ^www\.patricksanan\.org$
RewriteRule ^(.*)$ "https\:\/\/patricksanan\.org\/$1" [R=301,L]

# https://my.bluehost.com/hosting/help/force-ssl-all-pages
# has other suggestions..
